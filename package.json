{
  "name": "tt-truncate",
  "version": "1.1.4",
  "description": "A tiny, fast, ssr-friendly, zero-deps string truncate React component based on css.\n\n",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "size-limit": "size-limit",
    "test": "jest --testEnvironment=jsdom",
    "build": "rm -rf dist && parcel build",
    "ci": "npm install",
    "lint": "eslint src/*",
    "prepublishOnly": "npm run size-limit && npm run test && npm run build"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.19.4",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@size-limit/preset-small-lib": "8.1.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.2.0",
    "@types/react": "18.0.21",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "babel-jest": "29.2.1",
    "eslint": "8.22.0",
    "eslint-config-next": "13.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.1.3",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.2.1",
    "jest-environment-jsdom": "29.2.1",
    "parcel": "^2.7.0",
    "size-limit": "8.1.0",
    "typescript": "4.6.4"
  },
  "author": "t3hdrakkus@gmail.com",
  "license": "ISC",
  "source": "src/index.tsx",
  "size-limit": [
    {
      "limit": "600 B",
      "path": "src/index.tsx"
    }
  ]
}
