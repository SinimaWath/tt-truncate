{
  "extends": ["eslint:recommended", "prettier", "plugin:@typescript-eslint/recommended", "plugin:jest/recommended"],
  "plugins": ["prettier", "@typescript-eslint", "jest"],
  "rules": {
    "prettier/prettier": "error"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  }
}
